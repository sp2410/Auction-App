   <link href="https://fonts.googleapis.com/css?family=Great+Vibes|Baloo+Chettan|Playfair+Display+SC" rel="stylesheet"  lazyload>
    
    <!-- <link href="https://cdnjs.cloudflare.com/ajax/libs/card/1.3.1/js/card.min.js" lazyload> -->

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload'  %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload', :cache => true  %>

    <meta name="description" content="Auction your vehicle for free in real time, set custom time limit as per your time zone or post a vehicle now to see buyers interest level">  
    
    <link rel="canonical" href="http://www.tdcautoauction.com" />

